{
  "containerDefinitions": [
    {
      "name": "web-app",
      "image": "022499010637.dkr.ecr.us-east-1.amazonaws.com/web-app",
      "cpu": 512,
      "memory": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "name": "web-app-port",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/web-application-task-definition",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "web-application-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "taskRoleArn": "arn:aws:iam::022499010637:role/ecr-access-for-ecs",
  "executionRoleArn": "arn:aws:iam::022499010637:role/ecr-access-for-ecs",
  "ephemeralStorage": {
    "sizeInGiB": 200
  },
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
